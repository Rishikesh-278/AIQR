{
    "name": "AI-QRCodeCraft",
    "version": "1.0.0",
    "description": "Create beautiful AI-generated QR codes.",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html", 
        "default_title": "Create AI generated QR codes"
    },
    "permissions": [
        "activeTab",
        "management",
        "downloads"
    ],
    "background": {
        "service_worker": "background.js"
    }
}
